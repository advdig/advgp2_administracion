	<queryString>
		<![CDATA[SELECT
     (select nombre_comercial from datos_gasolinera limit 1) as nombrec,
     cliente.`cedula_ruc` AS cliente_cedula_ruc,
     cliente.`nombre` AS cliente_nombre,
	  IFNULL((select sum(total) from pagare,cliente where $P!{condicion}  and cliente_idcliente=idcliente and facturado=1 and cedula_ruc=cliente_cedula_ruc ), 0) as facturados,
	   IFNULL((select sum(total) from pagare,cliente where $P!{condicion} and cliente_idcliente=idcliente and  facturado=0 and cedula_ruc=cliente_cedula_ruc ), 0)  as no_facturados ,
IFNULL((SELECT sum(total)/pagare.punit FROM adv_facturacion.pagare,adv_facturacion.configuracion,adv_facturacion.producto,adv_facturacion.cliente where $P!{condicion} and  pagare.cliente_idcliente=idcliente and configuracion_nmanguera=nmanguera and producto_idproducto=idproducto and idproducto=1 and cedula_ruc=cliente_cedula_ruc  and pagare.facturado=1 and cedula_ruc=cliente_cedula_ruc group by idproducto ),0) as galones_extra,
IFNULL((SELECT sum(total) FROM adv_facturacion.pagare,adv_facturacion.configuracion,adv_facturacion.producto,adv_facturacion.cliente where  $P!{condicion} and pagare.cliente_idcliente=idcliente and configuracion_nmanguera=nmanguera and producto_idproducto=idproducto and idproducto=1 and pagare.facturado=1 and cedula_ruc=cliente_cedula_ruc group by idproducto),0) as total_extra
,
IFNULL((SELECT sum(total)/pagare.punit FROM adv_facturacion.pagare,adv_facturacion.configuracion,adv_facturacion.producto,adv_facturacion.cliente where $P!{condicion} and  pagare.cliente_idcliente=idcliente and configuracion_nmanguera=nmanguera and producto_idproducto=idproducto and idproducto=2 and pagare.facturado=1 and cedula_ruc=cliente_cedula_ruc  group by idproducto ),0) as galones_super,
IFNULL((SELECT sum(total) FROM adv_facturacion.pagare,adv_facturacion.configuracion,adv_facturacion.producto,adv_facturacion.cliente where  $P!{condicion} and pagare.cliente_idcliente=idcliente and configuracion_nmanguera=nmanguera and producto_idproducto=idproducto and idproducto=2 and pagare.facturado=1 and cedula_ruc=cliente_cedula_ruc  group by idproducto),0) as total_super,
IFNULL((SELECT sum(total)/pagare.punit FROM adv_facturacion.pagare,adv_facturacion.configuracion,adv_facturacion.producto,adv_facturacion.cliente where $P!{condicion} and  pagare.cliente_idcliente=idcliente and configuracion_nmanguera=nmanguera and producto_idproducto=idproducto and idproducto=3 and pagare.facturado=1 and cedula_ruc=cliente_cedula_ruc  group by idproducto ),0) as galones_diesel,
IFNULL((SELECT sum(total) FROM adv_facturacion.pagare,adv_facturacion.configuracion,adv_facturacion.producto,adv_facturacion.cliente where  $P!{condicion} and pagare.cliente_idcliente=idcliente and configuracion_nmanguera=nmanguera and producto_idproducto=idproducto and idproducto=3 and pagare.facturado=1 and cedula_ruc=cliente_cedula_ruc  group by idproducto),0)as total_diesel FROM
     `cliente` cliente INNER JOIN `credito_cliente` credito_cliente ON cliente.`idcliente` = credito_cliente.`cliente_idcliente`
 INNER JOIN `pagare` pagare ON cliente.`idcliente` = pagare.`cliente_idcliente`
where $P!{condicion} and pagare.facturado=1

group by cedula_ruc
order by  cliente.`nombre`]]>
	</queryString>
