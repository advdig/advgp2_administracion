	<subDataset name="facturas" uuid="dfd89f69-09e6-4219-8e10-667741e18518">
		<parameter name="condicion" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[select  
     TRUNCATE (factura_detalle_cantidad,3) AS factura_detalle_cantidad,
     TRUNCATE ( factura_total,3) AS  factura_total,
    metodo_pago AS metodo_pago,
    producto_nombre AS producto_nombre
from 

	(
	SELECT
    TRUNCATE(factura_detalle.`cantidad`,3) AS factura_detalle_cantidad,
    TRUNCATE (factura.`total`,3) AS factura_total,
     configuracion.`nmanguera` AS configuracion_nmanguera,
     producto.`nombre` AS producto_nombre,
	factura.fecha as fecha,
	factura.hora as hora,
	usuarios.nombre as nombre,
	factura.metodo_pago as metodo_pago
FROM
     `factura` factura INNER JOIN `factura_detalle` factura_detalle ON factura.`idfactura` = factura_detalle.`factura_idfactura`
     INNER JOIN `configuracion` configuracion ON factura_detalle.`configuracion_nmanguera` = configuracion.`nmanguera`
     INNER JOIN `producto` producto ON factura_detalle.`producto_idproducto` = producto.`idproducto`
     INNER JOIN `usuarios` usuarios ON factura.`usuarios_idusuarios` = usuarios.`idusuarios`
     INNER JOIN `tipo_producto` tipo_producto ON producto.`tipo_producto_idtipo_producto` = tipo_producto.`idtipo_producto`
where tipo_producto='Gasolina'


UNION all
SELECT
     TRUNCATE(pagare.`cantidad`,3) AS factura_detalle_cantidad,
     TRUNCATE (pagare.`total`,3) AS factura_total,
     pagare.`configuracion_nmanguera` AS configuracion_nmanguera,
     producto.`nombre` AS producto_nombre,
	 pagare.fecha as fecha,
	pagare.hora as hora,
	usuarios.nombre as nombre,
	'Credito' as metodo_pago
FROM
     `configuracion` configuracion INNER JOIN `pagare` pagare ON configuracion.`nmanguera` = pagare.`configuracion_nmanguera`
     INNER JOIN `producto` producto ON configuracion.`producto_idproducto` = producto.`idproducto`
     INNER JOIN `usuarios` usuarios ON pagare.`usuarios_idusuarios` = usuarios.`idusuarios`	
     INNER JOIN `cliente` cliente ON pagare.`cliente_idcliente` = cliente.`idcliente`
     INNER JOIN `credito_cliente` credito_cliente ON cliente.`idcliente` = credito_cliente.`cliente_idcliente`
INNER JOIN `tipo_producto` tipo_producto ON producto.`tipo_producto_idtipo_producto` = tipo_producto.`idtipo_producto`
where tipo_producto='Gasolina'

UNION all
SELECT
     TRUNCATE(pagare.`cantidad`,3) AS factura_detalle_cantidad,
     TRUNCATE(pagare.`total`,3) AS factura_total,
     pagare.`configuracion_nmanguera` AS configuracion_nmanguera,
     producto.`nombre` AS producto_nombre,
	 pagare.fecha as fecha,
	pagare.hora as hora,
	usuarios.nombre as nombre,
	'Prepago' as metodo_pago
FROM
     `configuracion` configuracion INNER JOIN `pagare` pagare ON configuracion.`nmanguera` = pagare.`configuracion_nmanguera`
     INNER JOIN `producto` producto ON configuracion.`producto_idproducto` = producto.`idproducto`
     INNER JOIN `usuarios` usuarios ON pagare.`usuarios_idusuarios` = usuarios.`idusuarios`	
     INNER JOIN `cliente` cliente ON pagare.`cliente_idcliente` = cliente.`idcliente`
     LEFT outer JOIN `credito_cliente` credito_cliente ON cliente.`idcliente` = credito_cliente.`cliente_idcliente`
	INNER JOIN `tipo_producto` tipo_producto ON producto.`tipo_producto_idtipo_producto` = tipo_producto.`idtipo_producto`
    where  tipo_producto='Gasolina' and credito_cliente.cupo_cliente is null and credito_cliente=1
)as A 
where  configuracion_nmanguera <> 0 and $P!{condicion}]]>
		</queryString>
		<field name="factura_detalle_cantidad" class="java.math.BigDecimal"/>
		<field name="factura_total" class="java.math.BigDecimal"/>
		<field name="metodo_pago" class="java.lang.String">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="producto_nombre" class="java.lang.String"/>
	</subDataset>
