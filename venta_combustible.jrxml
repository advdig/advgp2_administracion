	<subDataset name="cuadre" uuid="4456b871-d69a-4cea-80c5-4b0bab4353a9">
		<parameter name="condicion1" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<parameter name="condicion2" class="java.lang.String">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
     sum(reportes_usuario.`ventas`) AS  electonicos,
    IFNULL((SELECT sum(valor_dinero) FROM adv_facturacion.cuadre where $P!{condicion1}),0.00) as ventas,
   sum(reportes_usuario.`ventas`)-(SELECT sum(valor_dinero) FROM adv_facturacion.cuadre where $P!{condicion1}) as diferencia
    
FROM
     `usuarios` usuarios INNER JOIN `reportes_usuario` reportes_usuario ON usuarios.`idusuarios` = reportes_usuario.`usuarios_idusuarios`,
     `producto` producto INNER JOIN `configuracion` configuracion ON producto.`idproducto` = configuracion.`producto_idproducto`
WHERE
     configuracion.nmanguera = manguera
     and $P!{condicion1}]]>
		</queryString>
		<field name="electonicos" class="java.math.BigDecimal"/>
		<field name="ventas" class="java.math.BigDecimal">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="diferencia" class="java.math.BigDecimal"/>
	</subDataset>
