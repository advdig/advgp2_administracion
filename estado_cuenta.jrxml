	<subDataset name="New Dataset 1" uuid="2d4d2712-eade-4165-a477-06b4e9883819">
		<queryString language="SQL">
			<![CDATA[SELECT
     pagare.`total` AS pagare_total,
     pagare.`cantidad` AS pagare_cantidad,
     pagare.`fecha` AS pagare_fecha,
     pagare.`hora` AS pagare_hora,
     pagare.`idpagare` AS pagare_idpagare,
     pagare.`placa` AS pagare_placa,
     cliente.`cedula_ruc` AS cliente_cedula_ruc,
     factura.`numero` AS factura_numero
FROM
     `cliente` cliente INNER JOIN `pagare` pagare ON cliente.`idcliente` = pagare.`cliente_idcliente`
     INNER JOIN `factura` factura ON cliente.`idcliente` = factura.`cliente_idcliente`]]>
		</queryString>
