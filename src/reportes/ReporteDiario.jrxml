	<subDataset name="totalpagares" uuid="035f494a-2bf8-4a03-a334-768f133b794e">
		<parameter name="condicion" class="java.lang.String"/>
		<parameter name="totalp" class="java.lang.Double">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
      producto.`nombre` AS producto_nombre,   
      sum(pagare.`total`) AS pagare_total,  
      sum(pagare.`cantidad`) AS pagare_cantidad,
      count(pagare.`idpagare`) AS pagare_idpagare
     
FROM
     `configuracion` configuracion INNER JOIN `pagare` pagare ON configuracion.`nmanguera` = pagare.`configuracion_nmanguera`
     INNER JOIN `producto` producto ON configuracion.`producto_idproducto` = producto.`idproducto`
     INNER JOIN `usuarios` usuarios ON pagare.`usuarios_idusuarios` = usuarios.`idusuarios`		
where $P!{condicion}
group by producto.nombre]]>
		</queryString>
		<field name="producto_nombre" class="java.lang.String"/>
		<field name="pagare_total" class="java.math.BigDecimal"/>
		<field name="pagare_cantidad" class="java.math.BigDecimal"/>
		<field name="pagare_idpagare" class="java.lang.Long">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
	</subDataset>
	<subDataset name="totalfacturasContado" uuid="51c3c06f-6aa5-44ec-9a41-2d382b45d167">
		<parameter name="condicion" class="java.lang.String"/>
		<parameter name="totalc" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
     count(factura.`idfactura`) AS factura_idfactura,
     sum(factura_detalle.`cantidad`) AS factura_detalle_cantidad,
     sum(factura.`total`) AS factura_total,
     metodo_pago AS metodo,
     producto.`nombre` AS producto_nombre,
     tipo_producto.`tipo_producto` AS tipo_producto_tipo_producto
FROM
     `factura` factura INNER JOIN `factura_detalle` factura_detalle ON factura.`idfactura` = factura_detalle.`factura_idfactura`
     INNER JOIN `producto` producto ON factura_detalle.`producto_idproducto` = producto.`idproducto`
     INNER JOIN `tipo_producto` tipo_producto ON producto.`tipo_producto_idtipo_producto` = tipo_producto.`idtipo_producto`
     INNER JOIN `usuarios` usuarios ON factura.`usuarios_idusuarios` = usuarios.`idusuarios`
where $P!{condicion}
GROUP BY
     producto.nombre,metodo]]>
		</queryString>
		<field name="factura_idfactura" class="java.lang.Long">
			<fieldDescription><![CDATA[]]></fieldDescription>
		</field>
		<field name="factura_detalle_cantidad" class="java.math.BigDecimal"/>
		<field name="factura_total" class="java.math.BigDecimal"/>
		<field name="metodo" class="java.lang.String"/>
		<field name="producto_nombre" class="java.lang.String"/>
		<field name="tipo_producto_tipo_producto" class="java.lang.String"/>
	</subDataset>
	<subDataset name="totalfacturasCredito" uuid="2c73acdc-0b17-46ce-9167-f49bc5ec0d55">
		<parameter name="condicion" class="java.lang.String"/>
		<parameter name="totalcredito" class="java.lang.String"/>
		<queryString>
			<![CDATA[SELECT
     producto.`nombre` AS producto_nombre,     
    count( factura.`idfactura`) AS factura_idfactura,
    sum( factura_detalle.`cantidad`) AS factura_detalle_cantidad,
    sum( factura.`total`) AS factura_total
FROM
     `factura` factura INNER JOIN `factura_detalle` factura_detalle ON factura.`idfactura` = factura_detalle.`factura_idfactura`
     INNER JOIN `producto` producto ON factura_detalle.`producto_idproducto` = producto.`idproducto`
  INNER JOIN `usuarios` usuarios ON factura.`usuarios_idusuarios` = usuarios.`idusuarios`
where $P!{condicion} and metodo_pago='Credito'
group by producto.nombre]]>
