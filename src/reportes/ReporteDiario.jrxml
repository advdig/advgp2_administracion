	<subDataset name="notas" uuid="03dd465d-d994-42af-a2fd-1b4f05521fa0">
		<parameter name="cnotas" class="java.lang.String"/>
		<parameter name="totaln" class="java.lang.Double">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
     count(nota_credito.idnota_credito) AS notas,
     sum(nota_credito.`valor_modificacion`) AS nota_credito_valor_modificacion,
     nota_credito_detalle.`cantidad` AS nota_credito_detalle_cantidad,
     producto.`nombre` AS producto_nombre
FROM
     `nota_credito` nota_credito INNER JOIN `nota_credito_detalle` nota_credito_detalle ON nota_credito.`idnota_credito` = nota_credito_detalle.`nota_credito_idnota_credito`
     INNER JOIN `producto` producto ON nota_credito_detalle.`producto_idproducto` = producto.`idproducto`
     INNER JOIN `usuarios` usuarios ON nota_credito.`usuarios_idusuarios` = usuarios.`idusuarios`
where $P!{cnotas}
GROUP BY
     producto.`nombre`]]>
		</queryString>
